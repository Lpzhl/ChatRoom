<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="291.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RegisterController">
   <children>
      <ImageView fitHeight="499.0" fitWidth="782.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/邮箱登入.jpg" />
         </image>
      </ImageView>
      <Label layoutX="347.0" layoutY="8.0" text="用户注册" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="nickname" layoutX="221.0" layoutY="58.0" onAction="#nickname1" prefHeight="36.0" prefWidth="314.0" promptText="请输入用户名" />
      <TextField fx:id="Email" layoutX="221.0" layoutY="211.0" onAction="#Email1" prefHeight="36.0" prefWidth="314.0" promptText="请输入邮箱" />
      <Button fx:id="GetEmail" layoutX="406.0" layoutY="265.0" mnemonicParsing="false" onAction="#GetEmail1" prefHeight="36.0" prefWidth="130.0" style="-fx-background-color: #C0C0C0;" text="获取验证码" />
      <Button fx:id="Register" layoutX="222.0" layoutY="316.0" mnemonicParsing="false" onAction="#Register1" prefHeight="36.0" prefWidth="148.0" style="-fx-background-color: #3282F6;" text="注册" />
      <Button fx:id="cancel" layoutX="412.0" layoutY="316.0" mnemonicParsing="false" onAction="#cancel" prefHeight="36.0" prefWidth="124.0" style="-fx-background-color: #3282F6;" text="取消" />
      <TextField fx:id="PutCode" layoutX="221.0" layoutY="265.0" onAction="#PutCode1" prefHeight="36.0" prefWidth="157.0" promptText="请输入验证码" />
      <PasswordField fx:id="userpassword" layoutX="221.0" layoutY="107.0" onAction="#userpassword1" prefHeight="36.0" prefWidth="314.0" promptText="请输入密码" />
      <PasswordField fx:id="userpassword1" layoutX="221.0" layoutY="161.0" onAction="#userpassword1" prefHeight="36.0" prefWidth="314.0" promptText="请确认你的密码" />
   </children>
</AnchorPane>
